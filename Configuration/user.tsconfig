# This file is part of josefglatz/development-only which
# must only be loaded in non-productive environments

[applicationContext matches "#^Development#"]
  # Do not disable freshly pasted records
  setup.default.neverHideAtCopy = 1
  setup.override.neverHideAtCopy = 1

  # Disable Yoast SEO Page Module Include
  setup.override.hideYoastInPageModule = 1

  options.pageTree.showPageIdWithTitle = 1

  # Unset the requirement of mfa
  auth.mfa.required = 0
[global]
